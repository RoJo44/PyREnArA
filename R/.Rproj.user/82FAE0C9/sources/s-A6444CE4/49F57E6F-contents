source('RDA.R')

dir_name = '../../Linsel_Data/' # original folder of images

data_metrics = 'data_metrics_FD_clust' # name of csv table with data

filterlist = list('ID', 'X','X.1','class','tip_angle','ka_cal_BP','typology','region','clust','position_longest_extend')

df <- read.csv(paste0(dir_name,data_metrics,'.csv'))

rda(df,filterlist)
